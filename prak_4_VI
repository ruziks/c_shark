using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int sum=1+n;
            for (int i = 1; i*i<=n; i++)
            {
                if (n%i=0)
                {
                sum+=i;
                    if (i !== n/i){
                sum+=n/i
                }
            Console.WriteLine(sum);
            }
        }
    }
}
